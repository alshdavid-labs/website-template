{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM","ESNext"],
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./"
  }
}